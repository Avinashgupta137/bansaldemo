import platform
x= platform.system()
print(x)

def date(d,m,y):
    print("date: ",d,"/",m,"/",y )
date(23,4,2021)
date(22,4,2012)

#live date time
import datetime
x = datetime.datetime.now()
print(x)

x= min(3,5,7)
y=min(5,6,10)
print(x)
print(y)
print(x-y)

#power fun.
p = pow(5,2)
print("power", p)

#square root
import math
x = math.sqrt(99)
print(x)

x=math.ceil(1.4)
y = math.floor(1.4)
print(x)
print((y))

x=math.pi
print(x)
